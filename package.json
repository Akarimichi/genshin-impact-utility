{
    "name": "genshin-impact-utility",
    "productName": "Genshin impact utility",
    "version": "0.0.1",
    "description": "Genshin impact utility",
    "main": "build/index.js",
    "author": "Akarimichi",
    "license": "MIT",
    "scripts": {
        "lint": "eslint \"src/**/*.ts\"",
        "build": "cross-env NODE_ENV=production webpack --mode production",
        "start": "webpack-dev-server --port 9000"
    },
    "build": {
        "productName": "Acidary",
        "appId": "com.akarimichi.acidary",
        "files": [
            "main.js",
            "resources/**/*",
            "build/**/*",
            "!build/**/*.js.map",
            "!build/report.html"
        ],
        "directories": {
            "buildResources": "resources"
        },
        "afterSign": "scripts/electron-notarize.js",
        "mac": {
            "category": "public.app-category.developer-tools",
            "type": "distribution",
            "hardenedRuntime": true,
            "gatekeeperAssess": false,
            "entitlements": "assets/entitlements.mac.plist",
            "entitlementsInherit": "assets/entitlements.mac.plist"
        },
        "linux": {
            "category": "Development"
        },
        "win": {}
    },
    "dependencies": {
        "connected-react-router": "^6.8.0",
        "immer": "^7.0.7",
        "node-sass": "^4.14.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.1",
        "react-router-dom": "^5.2.0",
        "react-router-redux": "^4.0.8",
        "redux": "^4.0.5",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@types/node": "^14.0.27",
        "@types/react": "^16.9.44",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.7",
        "@types/react-router-redux": "^5.0.18",
        "@types/redux-logger": "^3.0.8",
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^7.0.2",
        "css-loader": "^4.2.1",
        "eslint": "^7.6.0",
        "file-loader": "^6.0.0",
        "fork-ts-checker-webpack-plugin": "^5.0.14",
        "hard-source-webpack-plugin": "^0.13.1",
        "html-webpack-plugin": "^4.3.0",
        "mini-css-extract-plugin": "^0.9.0",
        "prettier": "^2.0.5",
        "sass-loader": "^9.0.3",
        "source-map-loader": "^1.0.1",
        "style-loader": "^1.2.1",
        "ts-loader": "^8.0.2",
        "typed-css-modules-webpack-plugin": "^0.2.0",
        "typescript": "^3.9.7",
        "webpack": "^4.44.1",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    }
}
